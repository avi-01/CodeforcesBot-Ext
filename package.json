{
	"name": "codeforcesbot-ext",
	"displayName": "Codeforces Bot",
	"description": "Codeforces automation. Fetch, test and submit problems.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "codeforces-explorer",
					"title": "Codeforces Explorer",
					"icon": "res/img/mainIcon.svg"
				}
			]
		},
		"views": {
			"codeforces-explorer": [
				{
					"id": "contests",
					"name": "Contests",
					"icon": "res/img/mainIcon.svg",
					"contextualTitle": "Contests"
				},
				{
					"id": "rating",
					"name": "Rating",
					"icon": "res/img/mainIcon.svg",
					"contextualTitle": "Rating"
				}
			]
		},
		"commands": [
			{
				"command": "codeforcesbot-ext.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.21.0"
	}
}
